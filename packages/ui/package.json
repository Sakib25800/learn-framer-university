{
  "name": "@framer-university/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist/**",
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./theme.css": "./src/styles.css",
    "./styles.css": "./dist/styles.css",
    "./postcss.config": "./postcss.config.mjs"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "clean": "rm -rf dist",
    "dev": "pnpm tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/ --max-warnings 0",
    "test": "jest",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,md,json,html}\" --ignore-path ../../.prettierignore",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,md,json,html}\" --ignore-path ../../.prettierignore"
  },
  "jest": {
    "preset": "@framer-university/jest-presets/browser"
  },
  "dependencies": {
    "tailwind-merge": "^3.2.0",
    "class-variance-authority": "^0.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@framer-university/eslint-config": "workspace:*",
    "@framer-university/jest-presets": "workspace:*",
    "@framer-university/typescript-config": "workspace:*",
    "@swc/helpers": "^0.5.0",
    "@types/node": "^22.14.0",
    "bunchee": "^6.4.0",
    "eslint": "^9.25.0",
    "jest": "^29.7.0",
    "tailwindcss": "^4.1.4",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/cli": "^4.1.4",
    "concurrently": "^8.2.2",
    "tsup": "^7.2.0"
  },
  "peerDependencies": {
    "motion": ">=12.9.2",
    "@types/react": ">=19.1.4",
    "@types/react-dom": ">=19.1.5",
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  }
}

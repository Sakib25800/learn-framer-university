{
  "extends": "@framer-university/typescript-config/react-library.json",
  "compilerOptions": {
    "lib": ["dom", "ESNext"],
    "sourceMap": true,
    "types": ["jest", "node"],
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "strict": true
  },
  "include": ["src"],
  "exclude": ["dist", "build", "node_modules"]
}

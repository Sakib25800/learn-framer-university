{
  "extends": "@framer-university/typescript-config/base.json",
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "outDir": "./dist",
    "types": ["node"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "target": "ES2020",
    "allowImportingTsExtensions": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
